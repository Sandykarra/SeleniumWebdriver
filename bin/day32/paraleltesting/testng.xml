<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite  thread-count="2"   name="paralel_Suite" parallel="tests">
  <test name="edgeTest">
  <parameter name="browser" value="edge"/>
   <parameter name="app" value="https://www.google.co.uk/"/>
    <classes>
      <class name="day32.paraleltesting.ParalelTestingDemo"/>
    </classes>
  </test>  

 <test name="firefoxTest">
  <parameter name="browser" value="firefox"/>
   <parameter name="app" value="https://www.google.co.uk/"/>
    <classes>
      <class name="day32.paraleltesting.ParalelTestingDemo"/>
    </classes>
  </test> 


 <test name="chromeTest">
  <parameter name="browser" value="chrome"/>
          <parameter name="app" value="https://www.google.co.uk/"/>
    <classes>
      <class name="day32.paraleltesting.ParalelTestingDemo"/>
    </classes>
  </test> 
</suite> 